
<table id = "posttbl">
  <tr>
    <th>Name</th>
    <th>Average Score</th>
  </tr>
  <% Member.order("name ASC").where(inactive: [false, nil]).each do |member| %>
    <tr>
      <td><%= member.name %></td>
      <% avg(member.id) %>
      <td><%= @avg %></td>

    </tr>
  <% end %>
</table>
